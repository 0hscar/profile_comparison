# .env.example

# Needed in development
# OpenAI API key (required for AI features)
OPENAI_API_KEY=your-openai-api-key-here
# Serper API key (required for Google/Serper business data)
SERPER_API_KEY=your-serper-api-key-here


# /business_comparator/settings.py currently ignores these
# Django secret key (required) (not atm while development)
DJANGO_SECRET_KEY=your-django-secret-key-here
# Django debug mode (optional, set to 'True' for development)
DJANGO_DEBUG=True
# Allowed hosts (comma-separated, optional)
DJANGO_ALLOWED_HOSTS=localhost,127.0.0.1


# extra
# Database URL (optional, defaults to SQLite if not set)
# DATABASE_URL=postgres://user:password@localhost:5432/dbname

# CORS allowed origins (optional, comma-separated)
# CORS_ALLOWED_ORIGINS=http://localhost:8080

# Any other environment variables you need...
